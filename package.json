{
  "name": "octra-backend",
  "version": "0.2.0",
  "author": "Julian Pömp, Christoph Draxler; Institut für Phonetik und Sprachverarbeitung, LMU München",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && sh ./postinstall.sh",
    "start": "nx serve",
    "start:backend": "nx serve web-backend",
    "start:api": "npm run build:api && nx serve api",
    "build": "nx build",
    "build:backend": "nx build web-backend --prod && cp ./LICENSE ./dist/apps/web-backend/LICENSE",
    "build:api": "nx build api --generatePackageJson && node apps/api/src/app.prepare.js && cp ./LICENSE ./dist/apps/api/LICENSE",
    "build:cli": "nx build server-side && nx build cli --generatePackageJson && tsc -p apps/cli/tsconfig.assets.json && cp -rf dist/out-tsc/apps/cli/src/ dist/apps/cli/ && rm -rf node_modules/@octra && mkdir node_modules/@octra && cp -rf ./dist/libs/server-side ./node_modules/@octra/server-side && cp -rf ./dist/libs/api-types ./node_modules/@octra/api-types && cp ./LICENSE ./dist/apps/cli/LICENSE",
    "build:cli:prod": "nx build server-side && nx build cli --prod --generatePackageJson && tsc -p apps/cli/tsconfig.assets.prod.json && cp -rf dist/out-tsc/apps/cli/src/ dist/apps/cli/ && rm -rf node_modules/@octra && mkdir node_modules/@octra && cp -rf ./dist/libs/server-side ./node_modules/@octra/server-side && cp -rf ./dist/libs/api-types ./node_modules/@octra/api-types && cp ./LICENSE ./dist/apps/cli/LICENSE",
    "build:libs": "nx build octra-db && nx build ngx-api && cp ./LICENSE ./dist/libs/octra-db/LICENSE && cp ./LICENSE ./dist/libs/ngx-octra-api/LICENSE",
    "build:extern": "npm run build:libs && rm -rf extern && cp -rf dist/libs extern",
    "build:models": "rm -rf dist/api-models && ~/openapi-generator-cli generate -i dist/apps/api/swagger-spec.json -g typescript -o dist/api-models --skip-validate-spec --global-property skipFormModel=false && rm -rf libs/api-types/src/lib/models && mv -f dist/api-models/models libs/api-types/src/lib/",
    "watch:libs": "nx build ngx-octra-api --prod --watch & nx build octra-db --watch",
    "test:api": "nx test api --runInBand --bail true --silent",
    "test:api-nest": "nx e2e api-e2e",
    "deploy:api": "npm run package:api && sh ./deploy_dev.sh",
    "package:api": "nx build api --generatePackageJson --prod && node apps/api/src/app.prepare.js && pkg ./dist/apps/api/main.js --compress GZip -c ./dist/apps/api/package.json",
    "package:cli:prod": "pkg ./dist/apps/cli/main.js --compress GZip -c ./dist/apps/cli/package.json",
    "release:cli:after": "rm -rf node_modules/@octra",
    "release:cli:prod": "npm run build:cli:prod && npm run package:cli:prod && npm run release:cli:after",
    "test": "nx test",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "migrate": "nx migrate --run-migrations",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "lint": "nx workspace-lint && ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.0.0",
    "@angular/common": "14.0.0",
    "@angular/compiler": "14.0.0",
    "@angular/core": "14.0.0",
    "@angular/forms": "14.0.0",
    "@angular/platform-browser": "14.0.0",
    "@angular/platform-browser-dynamic": "14.0.0",
    "@angular/router": "14.0.0",
    "@fortawesome/angular-fontawesome": "^0.10.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
