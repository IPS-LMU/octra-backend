{
  "name": "octra-backend",
  "version": "0.2.0",
  "author": "Julian Pömp, Christoph Draxler; Institut für Phonetik und Sprachverarbeitung, LMU München",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "nx": "nx",
    "start": "ng serve",
    "start:backend": "nx serve octra-backend",
    "start:api": "npm run build:api && nx serve octra-api",
    "build": "ng build",
    "build:backend": "nx build octra-backend --prod && cp ./LICENSE ./dist/apps/octra-backend/LICENSE",
    "build:api": "nx build octra-api --generatePackageJson && node ./apps/octra-api/src/app.prepare.js && cp ./LICENSE ./dist/apps/octra-api/LICENSE",
    "build:libs": "nx build octra-db && nx build ngx-octra-api && cp ./LICENSE ./dist/libs/octra-db/LICENSE && cp ./LICENSE ./dist/libs/ngx-octra-api/LICENSE",
    "build:extern": "npm run build:libs && rm -rf extern && cp -rf dist/libs extern",
    "build:models": "rm -rf dist/api-models && ~/openapi-generator-cli generate -i dist/swagger-spec.json -g typescript -o dist/api-models",
    "watch:libs": "nx build ngx-octra-api --prod --watch & nx build octra-db --watch",
    "test:api": "nx test octra-api --runInBand --bail true --silent",
    "test:api-nest": "nx e2e octra-api-nest-e2e",
    "deploy:api-nest": "npm run package:api-nest && sh ./deploy_dev.sh",
    "package:api": "nx build octra-api --generatePackageJson --prod && node ./apps/octra-api/src/app.prepare.js && pkg ./dist/apps/octra-api/main.js -c ./dist/apps/octra-api/package.json",
    "package:api-nest": "nx build octra-api-nest --generatePackageJson --prod && node ./apps/octra-api-nest/src/app.prepare.js && pkg ./dist/apps/octra-api-nest/main.js -c ./dist/apps/octra-api-nest/package.json",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "migrate": "nx migrate --run-migrations",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help"
  },
  "private": true,
