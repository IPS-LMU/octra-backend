    "cors": "^2.8.4",
    "del": "^6.0.0",
    "ejs": "^3.1.6",
    "express": "^4.16.3",
    "fs-extra": "^9.1.0",
    "holderjs": "^2.9.9",
    "jquery": "^3.6.0",
    "jsonwebtoken": "^8.5.1",
    "lokijs": "^1.5.5",
