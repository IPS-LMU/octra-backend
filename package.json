    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@nestjs/common": "^8.4.6",
    "@nestjs/config": "^2.1.0",
    "@nestjs/core": "^8.4.6",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.4.6",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/throttler": "^2.0.1",
    "@nestjs/typeorm": "^8.1.2",
    "@nrwl/angular": "14.2.4",
    "bootstrap": "^5.1.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "clipboard": "^2.0.10",
    "codejar": "^3.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "del": "^6.0.0",
    "fs-extra": "^10.1.0",
    "helmet": "^5.0.2",
    "highlight.js": "^11.4.0",
    "holderjs": "^2.9.9",
    "jsonschema": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "jssha": "^3.2.0",
    "lokijs": "^1.5.5",
    "luxon": "^2.4.0",
    "mime-types": "^2.1.35",
    "music-metadata": "^7.12.3",
    "nestjs-form-data": "^1.6.3",
    "nestjs-redoc": "^2.2.2",
    "ngx-codejar": "^5.0.0",
    "ngx-webstorage": "^10.0.1",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "pkg": "^5.7.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "sqlite3": "^5.0.8",
    "swagger-ui-express": "^4.4.0",
    "tslib": "^2.0.0",
    "typeorm": "^0.3.6",
    "url-parse": "^1.5.10",
    "yargs": "^17.5.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.0.0",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "~14.0.0",
    "@angular/compiler-cli": "14.0.0",
    "@angular/language-service": "14.0.0",
    "@nestjs/schematics": "^8.0.9",
    "@nestjs/testing": "^8.4.3",
    "@nrwl/cli": "14.2.4",
    "@nrwl/cypress": "14.2.4",
