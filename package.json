    "@popperjs/core": "^2.9.1",
    "anchor-js": "^4.3.0",
    "bootstrap": "^4.6.0",
    "clipboard": "^2.0.8",
    "codejar": "^3.4.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "del": "^6.0.0",
    "express": "4.17.1",
    "fs-extra": "^9.1.0",
    "highlight.js": "^11.0.1",
    "holderjs": "^2.9.9",
    "jquery": "3.5.1",
    "jsonschema": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "lokijs": "^1.5.5",
    "luxon": "^1.27.0",
    "multer": "^1.3.0",
    "ngx-bootstrap": "^6.2.0",
