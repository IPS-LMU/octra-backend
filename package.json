{
  "name": "octra-backend",
  "version": "0.1.0",
  "author": "Julian Pömp, Christoph Draxler; Institut für Phonetik und Sprachverarbeitung, LMU München",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "nx": "nx",
    "start": "ng serve",
    "start:backend": "nx serve octra-backend",
    "start:api": "nx serve octra-api",
    "build": "ng build",
    "build:backend": "nx build octra-backend --prod && cp ./LICENSE ./dist/apps/octra-backend/LICENSE",
    "build:api": "nx build octra-api --generatePackageJson && node ./apps/octra-api/src/app.prepare.js && cp ./LICENSE ./dist/apps/octra-api/LICENSE",
    "build:libs": "nx build octra-db && nx build ngx-octra-api && cp ./LICENSE ./dist/libs/octra-db/LICENSE && cp ./LICENSE ./dist/libs/ngx-octra-api/LICENSE",
    "build:extern": "nx build octra-db --output-path extern/octra-db",
    "test:api": "DEBUG=jest nx test octra-api --runInBand --verbose false",
    "deploy:api": "npm run package:api && sh ./deploy.sh",
    "package:api": "nx build octra-api --generatePackageJson --prod && node ./apps/octra-api/src/app.prepare.js && pkg ./dist/apps/octra-api/main.js -c ./dist/apps/octra-api/package.json",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.0.0",
    "@angular/common": "^12.0.0",
    "@angular/compiler": "^12.0.0",
    "@angular/core": "^12.0.0",
    "@angular/forms": "^12.0.0",
    "@angular/platform-browser": "^12.0.0",
    "@angular/platform-browser-dynamic": "^12.0.0",
    "@angular/router": "^12.0.0",
    "@fortawesome/angular-fontawesome": "^0.8.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-regular-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@godaddy/terminus": "^4.7.0",
