{
  "version": 1,
  "projects": {
    "ngx-octra-api": {
      "projectType": "library",
      "root": "libs/ngx-octra-api",
      "sourceRoot": "libs/ngx-octra-api/src",
      "prefix": "octra-backend",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:ng-packagr-lite",
          "options": {
            "project": "libs/ngx-octra-api/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/ngx-octra-api/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/ngx-octra-api/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ngx-octra-api"],
          "options": {
            "jestConfig": "libs/ngx-octra-api/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ngx-octra-api/src/**/*.ts",
              "libs/ngx-octra-api/src/**/*.html"
            ]
          },
          "outputs": ["{options.outputFile}"]
        }
      },
      "tags": []
    },
    "octra-api": {
      "root": "apps/octra-api",
      "sourceRoot": "apps/octra-api/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:webpack",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/octra-api",
            "main": "apps/octra-api/src/main.ts",
            "tsConfig": "apps/octra-api/tsconfig.app.json",
            "assets": [
              "apps/octra-api/src/views",
              "apps/octra-api/src/static",
              "apps/octra-api/src/config.json"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/octra-api/src/environments/environment.ts",
                  "with": "apps/octra-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:node",
          "options": {
            "buildTarget": "octra-api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/octra-api/**/*.ts"]
          },
          "outputs": [
            "{options.outputFile}"
          ]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/octra-api"
          ],
          "options": {
            "jestConfig": "apps/octra-api/jest.config.js",
            "passWithNoTests": false,
            "bail": true,
            "runInBand": true
          }
        }
      },
      "tags": []
    },
    "octra-api-nest": {
      "root": "apps/octra-api-nest",
      "sourceRoot": "apps/octra-api-nest/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:webpack",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/octra-api-nest",
            "main": "apps/octra-api-nest/src/main.ts",
            "tsConfig": "apps/octra-api-nest/tsconfig.app.json",
            "assets": [
              "apps/octra-api-nest/src/assets",
              "apps/octra-api-nest/src/config.json",
              "apps/octra-api-nest/src/LICENSE"
            ],
            "port": 5321,
            "tsPlugins": [
              {
                "name": "@nestjs/swagger/plugin",
                "options": {
                  "classValidatorShim": false,
                  "introspectComments": true
                }
              }
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/octra-api-nest/src/environments/environment.ts",
                  "with": "apps/octra-api-nest/src/environments/environment.prod.ts"
                }
              ]
            },
            "dev": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:node",
          "options": {
            "buildTarget": "octra-api-nest:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/octra-api-nest/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/octra-api-nest"
          ],
          "options": {
            "jestConfig": "apps/octra-api-nest/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "octra-api-types": {
      "root": "libs/octra-api-types",
      "sourceRoot": "libs/octra-api-types/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "libs/octra-api-types/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/octra-api-types"
          ],
          "options": {
            "jestConfig": "libs/octra-api-types/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "octra-backend": {
      "projectType": "application",
      "root": "apps/octra-backend",
      "sourceRoot": "apps/octra-backend/src",
      "prefix": "ocb",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/octra-backend",
            "index": "apps/octra-backend/src/index.html",
            "main": "apps/octra-backend/src/main.ts",
            "polyfills": "apps/octra-backend/src/polyfills.ts",
            "tsConfig": "apps/octra-backend/tsconfig.app.json",
            "assets": [
              "apps/octra-backend/src/favicon.ico",
              "apps/octra-backend/src/assets",
              "apps/octra-backend/src/config"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/ngx-bootstrap/datepicker/bs-datepicker.css",
              "node_modules/highlight.js/styles/googlecode.css",
              "apps/octra-backend/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "3mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/octra-backend/src/environments/environment.ts",
                  "with": "apps/octra-backend/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "octra-backend:build:production"
            },
            "development": {
              "browserTarget": "octra-backend:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "octra-backend:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/octra-backend/src/**/*.ts",
              "apps/octra-backend/src/**/*.html"
            ]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/octra-backend"],
          "options": {
            "jestConfig": "apps/octra-backend/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "octra-backend-e2e": {
      "root": "apps/octra-backend-e2e",
      "sourceRoot": "apps/octra-backend-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/octra-backend-e2e/cypress.json",
            "tsConfig": "apps/octra-backend-e2e/tsconfig.e2e.json",
            "devServerTarget": "octra-backend:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "octra-backend:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/octra-backend-e2e/**/*.{js,ts}"]
          },
          "outputs": ["{options.outputFile}"]
        }
      },
      "tags": [],
      "implicitDependencies": ["octra-backend"]
    },
    "octra-db": {
      "root": "libs/octra-db",
      "sourceRoot": "libs/octra-db/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/octra-db/**/*.ts"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/octra-db"],
          "options": {
            "jestConfig": "libs/octra-db/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/js:tsc",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/libs/octra-db",
            "tsConfig": "libs/octra-db/tsconfig.lib.json",
            "packageJson": "libs/octra-db/package.json",
            "main": "libs/octra-db/src/index.ts",
            "assets": [
              "libs/octra-db/*.md"
            ]
          }
        }
      },
      "tags": []
    },
    "octra-api-nest-e2e": {
      "root": "apps/octra-api-nest-e2e",
      "sourceRoot": "apps/octra-api-nest-e2e/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:webpack",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/octra-api-nest-e2e",
            "main": "apps/octra-api-nest-e2e/src/main.ts",
            "tsConfig": "apps/octra-api-nest-e2e/tsconfig.app.json",
            "assets": [
              "apps/octra-api-nest-e2e/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/octra-api-nest-e2e/src/environments/environment.ts",
                  "with": "apps/octra-api-nest-e2e/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:node",
          "options": {
            "buildTarget": "octra-api-nest-e2e:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/octra-api-nest-e2e/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/octra-api-nest-e2e"
          ],
          "options": {
            "jestConfig": "apps/octra-api-nest-e2e/jest.config.js",
            "passWithNoTests": true
          }
        },
        "e2e": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/octra-api-nest-e2e/jest.config.js",
            "tsConfig": "apps/octra-api-nest-e2e/tsconfig.spec.json"
          }
        }
      },
      "tags": []
    }
  }
}
